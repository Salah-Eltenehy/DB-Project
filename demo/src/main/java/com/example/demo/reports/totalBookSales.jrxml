	<queryString language="SQL">
		<![CDATA[SELECT B.ISBN,B.TITLE,S.TOTAL_SALES 
	FROM BOOK AS B  JOIN (
							SELECT ISBN,SUM(AMOUNT) AS TOTAL_SALES
							FROM SALE 
                            WHERE sale_date > NOW() - INTERVAL 1 MONTH
							GROUP BY ISBN) AS S
							ON B.ISBN=S.ISBN;]]>
	</queryString>
