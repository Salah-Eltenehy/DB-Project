	<queryString language="SQL">
		<![CDATA[SELECT B.ISBN,B.TITLE,S.TOTAL_SALES
    FROM BOOK AS B JOIN(
					SELECT ISBN,SUM(amount) AS TOTAL_SALES
                    FROM SALE 
                    GROUP BY ISBN) AS S
					ON B.ISBN=S.ISBN
	ORDER BY S.TOTAL_SALES DESC
    LIMIT 10;]]>
	</queryString>
